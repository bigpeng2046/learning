{
  "name": "background-page",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Example for background page",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "19": "images/icon-19.png",
    "38": "images/icon-38.png"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
